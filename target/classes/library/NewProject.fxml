<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="800" prefWidth="1440" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cocky_fx.PrimaryController">

   <children>
      <Label text="COCKY CODERS" styleClass="loginSignUpTitle"/>

      <VBox id="loginVBox">

         <children>

            <Label text="Create New Project" styleClass="loginSignUpTitle"/>
            <Label text="New Project Name: " styleClass="labelToLeft"/>
            <TextField styleClass = "newProjectNameField"/>

            <Button text="Confirm" onAction="#ConfirmProjectButtonClicked" styleClass="signInSignUpButton"/>

         </children>
      </VBox>
   </children>
   
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>